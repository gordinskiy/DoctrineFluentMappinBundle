{
    "name": "gordinskiy/doctrine-fluent-mapping-bundle",
    "description": "Symfony Bundle for Fluent Mapping Driver from laravel-doctrine",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dmitriy Gordinskiy",
            "email": "dmitriy.gordinskiy94@gmail.com"
        }
    ],
    "require": {
        "doctrine/doctrine-bundle": "^2.6",
        "laravel-doctrine/fluent": "^1.3",
        "symfony/framework-bundle": "^6.0"
    },
    "require-dev": {
        "php": "^8.1",
        "ergebnis/composer-normalize": "^2.31",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^10.1",
        "psalm/plugin-symfony": "^5.0",
        "symfony/phpunit-bridge": "^6.2",
        "vimeo/psalm": "^5.11"
    },
    "autoload": {
        "psr-4": {
            "Gordinskiy\\DoctrineFluentMappingBundle\\": "src/",
            "Gordinskiy\\DoctrineFluentMappingBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
